package p11720;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int count = scanner.nextInt();
		
		int Numbers = scanner.nextInt();
		
		int tmp = 10;
		int sum = 0;
		
		for(int i = 0; i < count; i++) {
			
			int digit = Numbers%tmp;
			
			
			Numbers /= 10;
		}
	}

}
