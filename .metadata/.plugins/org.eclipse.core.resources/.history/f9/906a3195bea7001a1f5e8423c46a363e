package p1065;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
		int input = scanner.nextInt();
		int answer = 0;
		
		if (input<100) {
			answer = input;
		} else {
			
			
		}
		
		
		System.out.println(answer);
		
	}

}
