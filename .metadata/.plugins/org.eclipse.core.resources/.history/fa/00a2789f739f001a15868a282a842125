package p1056;

import java.util.ArrayList;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int X = sc.nextInt();
		
		for(int i = 0; i < X; i++) {
			int presentI = i;
			int cnt = 0;
			
			while(presentI!=0) {
				
				ArrayList<Integer> plate = new ArrayList<>();
				
				plate.add(presentI);
				
				presentI /= 10;
				
				if(plate.size()<=2) cnt++;
				
				else {
					for(int j = 0; j < 10; j++) {
						for(int k = 0;k < plate.size();k++) {
							
						}
					}
				}
			}
			
			
			
		}
	
	}

}
